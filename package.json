{
  "name": "manual-app",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "app to practice adding modules manually",
  "author": "Agustin Tamagno",
  "scripts": {
    "build": "node build.mjs",
    "build:dev": "esbuild src/index.ts --bundle --platform=node --target=node20 --sourcemap --outfile=dist/index.js --define:process.env.NODE_ENV='\"development\"'",
    "build:prod": "esbuild src/index.ts --bundle --platform=node --target=node20 --minify --outfile=dist/index.js --define:process.env.NODE_ENV='\"production\"'",
    "start": "node dist/index.js",
    "start:ts": "ts-node --transpile-only src/index.ts",
    "dev": "ts-node-dev --transpile-only --respawn src/index.ts",
    "dev:watch": "ts-node-dev --transpile-only --respawn --watch src src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "postinstall": "husky",
    "test": "jest"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  }
}
